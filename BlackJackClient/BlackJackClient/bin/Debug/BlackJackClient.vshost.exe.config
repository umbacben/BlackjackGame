<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <wsDualHttpBinding>
                <binding name="WSDualHttpBinding_IBlackjack" />
                <binding name="WSDualHttpBinding_IChat" />
                <binding name="WSDualHttpBinding_ILogin" />
            </wsDualHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8733/Design_Time_Addresses/BlackjackService/Service1/"
                binding="wsDualHttpBinding" bindingConfiguration="WSDualHttpBinding_IBlackjack"
                contract="ServiceReference1.IBlackjack" name="WSDualHttpBinding_IBlackjack">
                <identity>
                    <dns value="localhost" />
                </identity>
            </endpoint>
            <endpoint address="http://localhost:8734/Design_Time_Addresses/BlackjackService/Service1/"
                binding="wsDualHttpBinding" bindingConfiguration="WSDualHttpBinding_IChat"
                contract="ServiceReference1.IChat" name="WSDualHttpBinding_IChat">
                <identity>
                    <dns value="localhost" />
                </identity>
            </endpoint>
            <endpoint address="http://localhost:8733/Design_Time_Addresses/BlackjackService/Service1/"
                binding="wsDualHttpBinding" bindingConfiguration="WSDualHttpBinding_ILogin"
                contract="ServiceReference1.ILogin" name="WSDualHttpBinding_ILogin">
                <identity>
                    <dns value="localhost" />
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
</configuration>